<div id="<%= dom_id coin %>">
  <tr>
    <td><%= coin.description %></td>
    <td><%= coin.acronym %></td>
    <td><%= image_tag coin.url_image, width: 30, heigth: 30 %></td>
    <td>
      <%= link_to "Editar moeda", edit_coin_path(coin), class: "btn btn-primary" %> <%= button_to "Deletar moeda", coin, method: :delete, data: {confirm: "Tem certeza?"}, class: "btn btn-primary mt-1" %>
    </td>
  </tr>
</div>
